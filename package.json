{
  "name": "opencart-intellisense",
  "displayName": "OpenCart Intellisense",
  "description": "Provides intellisense for OpenCart files",
  "version": "0.0.1",
  "engines": {
      "vscode": "^1.85.0"
  },
  "categories": [
      "Programming Languages"
  ],
  "activationEvents": [
      "onLanguage:php"
  ],
  "main": "./out/extension.js",
  "contributes": {
      "languages": [{
          "id": "php",
          "extensions": [".php"]
      }]
  },
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "make": "vsce package"
  },
  "devDependencies": {
      "@types/node": "^16.11.7",
      "@types/vscode": "^1.85.0",
      "typescript": "^4.9.5"
  }
}